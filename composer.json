{
	"name": "sitole/toaster",
	"authors": [
		{
			"name": "Jiří Svěcený",
			"homepage": "https://sitole.cz"
		}
	],
	"type": "project",
	"require": {
		"php": ">= 7.2",
		"ext-openssl": "*",
		"ext-mbstring": "*",

		"nette/application": "^2.4",
		"nette/bootstrap": "^2.4",
		"nette/caching": "^2.4",
		"nette/di": "^2.4",
		"nette/http": "^2.4",
		"nette/mail": "^2.4",
		"nette/security": "^2.4",
		"nette/utils": "^2.4",
		"latte/latte": "^2.5",
		"tracy/tracy": "^2.6",

		"symfony/dom-crawler": "^4.2",
		"symfony/css-selector": "^4.2",

		"nextras/migrations": "^3.0",
		"nextras/orm": "^3.1",
		"contributte/console": "^0.5.0"
	},
	"require-dev": {
		"phpstan/phpstan-nette": "^0.11",
		"nextras/orm-phpstan": "^0.3"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app"
		}
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"scripts": {
		"phpstan": [
			"vendor/bin/phpstan analyse app -c phpstan.neon --level 6 --no-progress"
		]
	}
}
